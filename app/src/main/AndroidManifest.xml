    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TaskList" />
        <activity android:name=".ReminderList" />
        <activity android:name=".HabitList" />
        <activity android:name=".Setting" />
        <activity android:name=".plusTask" />
        <activity android:name=".plusReminder" />
        <activity android:name=".plusHabit" />
        <activity android:name=".LeftReminder" />
        <activity android:name=".LeftTask" />
        <activity android:name=".LeftHabit" />
        <activity android:name=".AboutMe" />
        <activity android:name=".Register" />
        <activity android:name=".Login" />
        <activity android:name=".remindMe" />
        <activity android:name=".ChooseProfile"></activity>
    </application>

</manifest>
